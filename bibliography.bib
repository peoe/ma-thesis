   keywords = {Model order reduction,Multivariate rational interpolation,Nonlinear control systems,Stability preservation,Tensor calculus,nonlinear control systems,stability preservation,tensor calculus AMS subject classifications 93A15},
   pages = {B239-B260},
   publisher = {Society for Industrial and Applied Mathematics Publications},
   title = {Two-sided projection methods for nonlinear model order reduction},
   volume = {37},
   url = {http://www.siam.org/journals/sisc/37-2/97255.html},
   year = {2015},
}
